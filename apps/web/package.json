{
  "name": "my-qwik-empty-starter",
  "description": "Blank project with routing included",
  "engines": {
    "node": "^18.17.0 || ^20.3.0 || >=21.0.0"
  },
  "engines-annotation": "Mostly required by sharp which needs a Node-API v9 compatible runtime",
  "private": true,
  "type": "module",
  "scripts": {
    "docker:dev": "bun ./scripts/docker-dev-proxy.cjs",
    "docker:prod": "bun ./scripts/docker-prod-proxy.cjs",
    "build": "bunx vite build",
    "build:client": "bunx vite build",
    "build.types": "bunx -p typescript@5.9.2 tsc --incremental --noEmit",
    "dev.debug": "bun --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "dev.public": "vite --mode ssr --host 0.0.0.0",
    "dev:web": "bun ../../scripts/dev-env.js",
    "dev:web:nohmr": "bun ../../scripts/dev-nohmr.js",
    "fmt:fix": "bunx -p prettier@3.6.2 prettier --write \"**/*.{md,json,css,scss,html,yaml,yml}\"",
    "fmt:check": "bunx -p prettier@3.6.2 prettier --check \"**/*.{md,json,css,scss,html,yaml,yml}\"",
    "gateway:dev": "bun ../../scripts/start-gateway.js",
    "lynx:auto": "bun ../../scripts/wait-and-launch.js",
    "build:ssg": "bunx vite build && bunx vite build -c adapters/static/vite.config.ts",
    "test.e2e": "bunx -p @playwright/test@1.55.0 playwright test",
    "biome:check": "biome check src --max-diagnostics=500",
    "imports:fix": "biome check --write src --max-diagnostics=500",
    "biome:fix": "biome check --write src --max-diagnostics=500"
  },
  "devDependencies": {
    "@builder.io/qwik": "1.16.0",
    "@builder.io/qwik-city": "1.16.0",
    "@builder.io/qwik-react": "0.5.6",
    "@eslint/js": "9.35.0",
    "@modular-forms/qwik": "0.29.1",
    "@playwright/test": "1.55.0",
    "@preact/preset-vite": "2.10.2",
    "@tailwindcss/postcss": "4.1.13",
    "@tailwindcss/vite": "4.1.13",
    "@types/bun": "1.2.22",
    "@types/node": "24.4.0",
    "autoprefixer": "10.4.21",
    "concurrently": "9.2.1",
    "eslint": "9.35.0",
    "eslint-plugin-qwik": "1.16.0",
    "globals": "16.4.0",
    "postcss": "8.5.6",
    "prettier": "3.6.2",
    "prettier-plugin-tailwindcss": "0.6.14",
    "rollup-plugin-visualizer": "6.0.3",
    "selfsigned": "3.0.1",
    "tailwindcss": "4.1.13",
    "tailwindcss-animate": "1.0.7",
    "typescript": "5.9.2",
    "typescript-eslint": "8.43.0",
    "typescript-plugin-css-modules": "5.2.0",
    "vite": "7.1.5",
    "vite-plugin-compression2": "2.2.1",
    "vite-plugin-mkcert": "1.17.8",
    "vite-tsconfig-paths": "5.1.4",
    "unplugin-icons": "22.3.0",
    "@iconify-json/lucide": "1.2.68",
    "@svgr/core": "8.1.0",
    "@svgr/plugin-jsx": "8.1.0",
    "preact-render-to-string": "6.6.1",
    "@unpic/qwik": "1.0.0",
    "@biomejs/biome": "2.2.4",
    "knip": "5.63.1",
    "vite-plugin-inspect": "11.3.3",
    "unplugin-fonts": "1.4.0",
    "msw": "2.11.2",
    "vite-imagetools": "8.0.0",
    "lightningcss": "1.30.1",
    "lightningcss-wasm": "1.30.1"
  },
  "dependencies": {
    "@faker-js/faker": "10.0.0",
    "@stack/shared": "workspace:*",
    "daisyui": "5.1.12",
    "motion": "12.23.12",
    "ofetch": "1.4.1",
    "preact": "10.27.2",
    "tailwind-merge": "3.3.1",
    "valibot": "1.1.0",
    "velvette": "0.1.8"
  }
}
