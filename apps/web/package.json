{
  "name": "my-qwik-empty-starter",
  "description": "Blank project with routing included",
  "engines": {
    "node": "^18.17.0 || ^20.3.0 || >=21.0.0"
  },
  "engines-annotation": "Mostly required by sharp which needs a Node-API v9 compatible runtime",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/static/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "build:static": "vite build -c adapters/static/vite.config.ts",
    "deploy": "echo 'Run \"npm run qwik add\" to install a server adapter'",
    "dev": "concurrently -n GW,VITE,LYNX -c \"yellow.bold,cyan.bold,magenta.bold\" \"bun run gateway:dev\" \"bun run dev:web\" \"bun run lynx:auto\"",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "dev.panda": "panda --watch",
    "dev.public": "vite --mode ssr --host 0.0.0.0",
    "dev:lynx": "bun run dev",
    "dev:web": "node ./scripts/dev-with-env.js",
    "dev:web:nohmr": "node ./scripts/dev-nohmr.js",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "gateway:dev": "node ../../scripts/start-gateway.js",
    "lint": "eslint \"src/**/*.ts*\"",
    "check:auth-debug": "node ./scripts/check-auth-debug.cjs",
    "lynx:auto": "node ./scripts/wait-and-launch.js",
    "panda": "panda",
    "panda.build": "panda build",
    "panda.codegen": "panda codegen",
    "preview": "qwik build preview && vite preview --open",
    "preview.public": "qwik build preview && vite preview --host 0.0.0.0",
    "dev:ssr": "node ./scripts/dev-with-env.js",
    "build:ssg": "bunx vite build && bunx vite build -c adapters/static/vite.config.ts",
    "build:ssg:watch": "bunx concurrently -n client,ssg -c \"cyan,magenta\" \"bunx vite build --watch\" \"bunx vite build -c adapters/static/vite.config.ts --watch\"",
    "preview:ssg": "bunx vite preview --host 0.0.0.0 --strictPort --port 5174 --outDir dist",
    "preview:ssg:watch": "bunx concurrently -n watch,preview -c \"magenta,cyan\" \"bun run build:ssg:watch\" \"bunx wait-on ./dist/index.html && bunx vite preview --host 0.0.0.0 --strictPort --port 5174 --outDir dist\"",
    "serve": "bun server/entry.bun.js",
    "start": "vite --open --mode ssr",
    "test.e2e": "playwright test",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@auth/qwik": "^0.8.0",
    "@builder.io/qwik": "^1.16.0",
    "@builder.io/qwik-city": "^1.16.0",
    "@eslint/js": "latest",
    "@modular-forms/qwik": "0.29.1",
    "@pandacss/dev": "^1.2.0",
    "@pandacss/preset-panda": "^1.2.0",
    "@playwright/test": "1.55.0",
    "@qwik.dev/partytown": "0.11.2",
    "@qwikest/icons": "^0.0.13",
    "@tailwindcss/postcss": "^4.1.12",
    "@tailwindcss/vite": "4.1.12",
    "@types/bun": "^1.2.21",
    "@types/node": "24.3.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "eslint": "9.34.0",
    "eslint-plugin-qwik": "^1.16.0",
    "flowbite-qwik": "^1.1.2",
    "globals": "16.3.0",
    "postcss": "^8.5.6",
    "prettier": "3.6.2",
    "prettier-plugin-tailwindcss": "0.6.14",
    "qrcode-terminal": "^0.12.0",
    "rollup-plugin-visualizer": "^6.0.3",
    "solid-js": "^1.9.9",
    "tailwindcss": "4.1.13",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "5.9.2",
    "typescript-eslint": "8.42.0",
    "typescript-plugin-css-modules": "latest",
    "undici": "^7.15.0",
    "vite": "7.1.4",
    "vite-plugin-solid": "^2.11.8",
    "vite-plugin-mkcert": "1.17.8",
    "selfsigned": "3.0.1",
    "vite-tsconfig-paths": "5.1.4",
    "wait-on": "^8.0.4"
  },
  "dependencies": {
    "@stack/shared": "workspace:*",
    "@auth/core": "^0.40.0",
    "@builder.io/partytown": "^0.10.3",
    "@faker-js/faker": "^10.0.0",
    "@lottiefiles/lottie-player": "^2.0.12",
    "daisyui": "5.1.7",
    "framer-motion": "^12.23.12",
    "htmx.org": "^2.0.6",
    "lenis": "1.3.11",
    "lottie-web": "^5.13.0",
    "motion": "^12.23.12",
    "html2canvas": "^1.4.1",
    "ofetch": "^1.4.1",
    "valibot": "^1.1.0",
    "zod": "4.1.5"
  }
}
